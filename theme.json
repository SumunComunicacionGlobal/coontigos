		"custom": {
			"border-radius": {
				"lg": "16px",
				"md": "8px",
				"rounded": "999px",
				"sm": "4px",
				"xl": "32px",
				"zero": "0"
			},
			"border-width": {
				"large": "4px",
				"medium": "3px",
				"small": "2px",
				"tiny": "1px"
			},
			"box-shadow": {
				"1": "0px 2px 8px rgba(33, 33, 33, 0.12)",
				"2": "3px 3px 10px 10px rgba(0, 0, 0, 0.10)"
			},
			"color": {
				"link": "var(--wp--preset--color--primary)",
				"neutral-black": "#000000",
				"neutral-white": "#FFFFFF"
			},
			"layout": {
				"block-gap": "2rem",
				"block-gap-large": "2.5rem",
				"content": "1216px",
				"padding": "16px",
				"page": "var(--wp--custom--layout--content)",
				"sidebar": "336px",
				"wide": "90%"
			},
			"letter-spacing": {
				"loose": ".05em",
				"looser": ".1em",
				"none": "normal",
				"tight": "-.01em"
			},
			"line-height": {
				"heading": 1.1,
				"normal": 1.5,
				"tiny": 1
			}
		},
		"layout": {
			"contentSize": "var(--wp--custom--layout--content)",
			"wideSize": "var(--wp--custom--layout--wide)"
		},
		"spacing": {
			"defaultSpacingSizes": false,
			"spacingSizes": [
				{
					"name": "Space 1 (1rem - 16px)",
					"size": "1rem",
					"slug": "10"
				},
				{
					"name": "Space 2 (1.5rem - 24px)",
					"size": "1.5rem",
